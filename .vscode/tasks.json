{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build Module",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "build",
                "${workspaceFolder}/Source/Module/Module.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build Module.Test",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "build",
                "${workspaceFolder}/Test/Module.Test/Module.Test.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "test Module.Test",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "test",
                "${workspaceFolder}/Test/Module.Test/Module.Test.csproj"
            ],
            "problemMatcher": "$msCompile",
            "dependsOn": "build Module.Test"
        },
        {
            "label": "build",
            "dependsOn": [
                "build Module",
                "build Module.Test"
            ],
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
